<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Project Listing</title>
  </head>
  <body>
    <%- include('../shared/header.ejs'); -%>
    <main>
      <h2 style="text-align: center">Select Project to explore</h2>
      <h2>Project List</h2>
      <ul>
      <%
      PAGE_LIST.map(
        function(page){
           %> <li  style="width: fit-content;"><a href="<%- page.path -%>"> <h3><%- page.title -%></h3></a></li>
      <% }) %>
      </ul>
    </main>
    <footer></footer>
  </body>
</html>
